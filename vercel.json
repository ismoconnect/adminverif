{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/admin/notifications",
      "dest": "https://notifications.vercel.app/"
    },
    {
      "src": "/admin/notifications/(.*)",
      "dest": "https://notifications.vercel.app/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
